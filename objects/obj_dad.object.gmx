          <argument>
            <kind>1</kind>
            <string>///Get the player's input

key_right = keyboard_check(vk_right); // 0 or 1
//key_right = keyboard_check(ord("D"));
key_left = - keyboard_check(vk_left); // -(1 or 0)

key_jump = keyboard_check_pressed(vk_up);

key_down = keyboard_check(vk_down);
//React to inputs
move = key_left + key_right;

hsp = move * movespeed;

if(vsp &lt; 10)
{
vsp += grav;
}

if(place_meeting(x,y+1,obj_wall)||place_meeting(x,y+1,obj_onewaybar))
{

// vsp = key_jump * -jumpspeed;

//if( key_jump) {vsp = -jumpspeed;}

airjumps= jumpsmax;

}
if(place_meeting(x,y+1,obj_trap))
{

// vsp = key_jump * -jumpspeed;

//if( key_jump) {vsp = -jumpspeed;}

airjumps= jumpsmax;

}

if((key_jump) &amp;&amp; (airjumps&gt;0)) 
{

audio_play_sound(sou_Jump,0,false)
airjumps-=1;
vsp = -jumpspeed;
}


//wall jump
/*
if((key_jump) &amp;&amp;  (place_meeting(x+1,y,obj_wall)||place_meeting(x-1,y,obj_wall))) 
{
    vsp = -jumpspeed;
}
*/
hsp_final = hsp + hsp_carry;
hsp_carry=0;

//horizantal collusion
if(place_meeting(x+hsp_final,y,obj_wall))
{

    while(!place_meeting(x+sign(hsp_final),y,obj_wall))
        {
            x+=sign(hsp_final);
        }
        hsp_final=0;
       hsp=0;
}



//vertical collusion
if(place_meeting(x,y+vsp,obj_wall))
{

    while(!place_meeting(x,y+sign(vsp),obj_wall))
        {
            y+=sign(vsp);
        }
        vsp=0;
}

////////////////////////////////////////////////////////////
else{
//vertical collusion // newwww

if(place_meeting(x,y+vsp,obj_onewaybar))
{

    while(!place_meeting(x,y+sign(vsp),obj_onewaybar))
        {
            y+=sign(vsp);
        }
        vsp=0;
}


}
///////////////////////////////////////////////////////////////


//Horizental collision trap
if(place_meeting(x+hsp_final,y,obj_trap))
{
    while(!place_meeting(x+sign(hsp_final),y,obj_trap))
        {x+=sign(hsp_final);}
        hsp_final=0;
    hsp=0;
}
//Vertical collision trap
if(place_meeting(x,y+vsp,obj_trap))
{
    while(!place_meeting(x,y+sign(vsp),obj_trap))
        {y+=sign(vsp);}
      var o=instance_place(x,y+sign(vsp),obj_trap);
     if(o.alarm[0]= -1) o.alarm[0] = 3
    vsp=0;
}


x += hsp_final;
y += vsp;

// set the sprite 

if(keyboard_check(vk_space)){

sprite_index = spr_dad_kick

}else if(keyboard_check(vk_right)&amp;&amp; (!keyboard_check(vk_up))&amp;&amp; (!keyboard_check(vk_space)))
{
sprite_index = dad_run
image_xscale=abs(image_xscale)
dir=1;//new

}else if(keyboard_check(vk_left)&amp;&amp; (!keyboard_check(vk_up))&amp;&amp; (!keyboard_check(vk_space))){
sprite_index = dad_run
image_xscale=-1*abs(image_xscale)
dir=-1;//new

}else if(keyboard_check(vk_up)&amp;&amp; (!keyboard_check(vk_space))) {

sprite_index = dad_jump

}else if(!keyboard_check(vk_space)){
sprite_index = dad_pause

}


// death

if(hp&lt;=0){
audio_play_sound(sou_dead,0,false)
scr_death();
}



   if(shield==1)
     {
        effect_create_below(ef_ring,x-10,y-10,1,c_gray)
        
     }

</string>
